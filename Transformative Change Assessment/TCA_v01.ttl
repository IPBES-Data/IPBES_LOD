    ipbes:hasEstablishedIncomplete "Established_but_incomplete" ;
    dcterms:identifier "B6" ;
    skos:prefLabel "Redefining goals, metrics and indicators to acknowledge economic, social (including cultural) and environmental dimensions as well as the many different values of nature can promote new paradigms of progress that centre on justice and sustainability (strategy 3) (established but incomplete) {4.4.2, 5.5.4, 5.5.3, 5.6.3}. " .

<http://ontology.ipbes.net/report/bgm/TCA24-C6> a ipbes:BackgroundMessage ;
    ipbes:Report <http://ontology.ipbes.net/report/TCA24> ;
    ipbes:SubMessage <http://ontology.ipbes.net/report/subm/TCA24-C6-SM0>,
        <http://ontology.ipbes.net/report/subm/TCA24-C6-SM1>,
        <http://ontology.ipbes.net/report/subm/TCA24-C6-SM2>,
